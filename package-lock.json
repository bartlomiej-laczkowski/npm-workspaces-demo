{
  "name": "npm-workspaces-demo",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "version": "1.0.0",
      "license": "MIT",
      "workspaces": [
        "./packages/*"
      ]
    },
    "node_modules/dep-a": {
      "resolved": "packages/a",
      "link": true
    },
    "node_modules/dep-b": {
      "resolved": "packages/b",
      "link": true
    },
    "packages/a": {
      "version": "1.0.0"
    },
    "packages/b": {
      "version": "1.0.0",
      "dependencies": {
        "dep-a": "1.0.0"
      }
    }
  },
  "dependencies": {
    "dep-a": {
      "version": "file:packages/a"
    },
    "dep-b": {
      "version": "file:packages/b",
      "requires": {
        "dep-a": "1.0.0"
      }
    }
  }
}
